package com.jlcindia.hibernate;

public class Customer {

	private String cid;
	private String cname;
	private String email;
	private long phone;
	
	
	public Customer() {}
	
	public Customer(String cname, String email, long phone) {
	
		this.cname = cname;
		this.email = email;
		this.phone = phone;
	}
	
	
	
	
	
	
}
